{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi":"warn",
        "no-empty":"warn",
        "indent":"warn",
        "no-trailing-spaces": "off",
        "keyword-spacing":"off"
    }
}
